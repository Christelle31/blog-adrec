{% extends 'base.html.twig' %}

{% block title %}Hello ListArticleController!{% endblock %}

{% block body %}
<ul>
    {% for article in articles %}
    <li>
        <a href="{{ path('show_one_article', {'id': article.id}) }}">
            {{ article.id }} - {{ article.title }}
        </a>
    </li>
    {% endfor %}
</ul>
{% endblock %}
